{
  "name": "Screensharing Sample",
  "description": "Sample chrome extension for screen sharing",
  "version": "0.0.1",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "icons": {
  },
  "permissions": [
    "desktopCapture"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "https://example.com/*"
      ]
    }
  ],
  "externally_connectable": {
    "matches": [
      "https://example.com/*"
    ]
  }
}
